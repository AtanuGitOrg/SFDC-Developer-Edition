        <protected>false</protected>
        <url>if(&apos;{!Cloud_Storage__c.RecordType}&apos; == &apos;Folder&apos;){
alert(&apos;Custom view is not available for folders&apos;);
}
else{
var furl =document.domain;
var ffurl = furl.split(&apos;\.&apos;);
var clouddrop_RelateTo = &quot;{!Cloud_Storage__c.Relate_To__c}&quot;;
var clouddrop_ObjectId=&quot;{!Cloud_Storage__c.ObjectId__c}&quot;;
var staticURL = &quot;https://cloudrop.&quot;+ffurl[0]+&quot;.visual.force.com/apex/Viewfilepage_Dynamic?FileId={!Cloud_Storage__c.Id}&amp;RecId=&quot;;
if(clouddrop_ObjectId==&apos;&apos;)
{
staticURL = staticURL+clouddrop_RelateTo;
}
else
{
staticURL = staticURL+clouddrop_ObjectId;
}
location.replace(staticURL);
}</url>
